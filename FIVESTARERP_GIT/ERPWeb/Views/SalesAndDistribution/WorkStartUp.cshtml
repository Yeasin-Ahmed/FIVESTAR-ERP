            var brandCategories = [];
            if ($.trim(categories) !== "" && categories.indexOf(",") < 0) {
                brandCategories.push(categories)
            }
            else if ($.trim(categories) !== "" && categories.indexOf(",") >= 0) {
                brandCategories= $.trim(categories).split(",");
