@section scripts{
    <script type="text/javascript">

        // Dealer
        var hdfDealerId = $("#hdfDealerId");
        var txtDealerName = $("#txtDealerName");
        var txtAddress = $("#txtAddress");
        var txtTelephone = $("#txtTelephone");
        var txtMobile = $("#txtMobile");
        var txtEmail = $("#txtEmail");
        var txtContactPersonName = $("#txtContactPersonName");
        var txtContactPersonMobile = $("#txtContactPersonMobile");
        var txtRemarks = $("#txtRemarks");
        var chkStatestatus = $("#chkStatestatus");

        // Category
        var hdfCategoryId = $("#hdfCategoryId");
        var chkCategoryStatestatus = $("#chkCategoryStatestatus");
        var txtCategoryName = $("#txtCategoryName");
        var txtCategoryRemarks = $("#txtCategoryRemarks");

        $(document).ready(function () {
            loadDataInitializer();
        })

        function loadDataInitializer() {
            loadDealerDataTable();
            loadCategoryDataTable();
        }

        function loadDealerDataTable() {
            $("#searchByDealer").val('');
            var data = { flag: "dealer" };
            $.when(getReqWithData(dataType.html, type.get, '/SalesAndDistribution/WorkStartUp', data)).then(function (res, status) {
                if (status === "success") {
                    $("#dataContainer1").fadeOut('500', function () {
                        $("#dataContainer1").empty();
                        $("#dataContainer1").append(res).fadeIn('500');
                    });
                }
            }).fail(function (error) {
                consoleLog(error);
            })
        }

        function loadCategoryDataTable() {
            $("#searchByCategory").val('');
            var data = { flag: "category" };
            $.when(getReqWithData(dataType.html, type.get, '/SalesAndDistribution/WorkStartUp', data)).then(function (res, status) {
                if (status === "success") {
                    $("#dataContainer2").fadeOut('500', function () {
                        $("#dataContainer2").empty();
                        $("#dataContainer2").append(res).fadeIn('500');
                    });
                }
            }).fail(function (error) {
                consoleLog(error);
            })
        }

        $("#btnAddNewDealer").click(function (e) {
            e.preventDefault();
            OpenModalDealer();
        })

        function OpenModalDealer() {
            $("#modalDealer").modal("show");
            $("#spanModalHeadDealer").text("Add New Dealer");
            $("#spanSaveTextDealer").text("Save");
            clearDealerCtrl();
        }

        function clearDealerCtrl() {
            hdfDealerId.val('0');
            txtDealerName.val('');
            txtAddress.val('');
            txtTelephone.val('');
            txtMobile.val('');
            txtEmail.val('');
            txtContactPersonName.val('');
            txtContactPersonMobile.val('');
            txtRemarks.val('');
            chkStatestatus.prop("checked", false);
            txtDealerName.focus();
        }

        function validateDealer() {
            var isValid = true;
            $(".error").addClass("hide");
            if ($.trim(txtDealerName.val()) == '') {
                $(".required-dealerName").removeClass("hide");
                isValid = false;
            }
            if ($.trim(txtAddress.val()) == '') {
                $(".required-address").removeClass("hide");
                isValid = false;
            }
            return isValid;
        }

        $(document).on('click', '.data-dealer-edit', function (e) {
            e.preventDefault();
            OpenModalDealer();
            $("#spanModalHeadDealer").text("Update Dealer");
            $("#spanSaveTextDealer").text("Update");
            var tds = $(this).parent().parents('tr').children('td');
            hdfDealerId.val(tds.eq(1).html());
            txtDealerName.val(tds.eq(2).html());
            txtAddress.val(tds.eq(3).html());
            txtTelephone.val(tds.eq(4).html());
            txtMobile.val(tds.eq(5).html());
            txtEmail.val(tds.eq(6).html());
            txtContactPersonName.val(tds.eq(7).html());
            txtContactPersonMobile.val(tds.eq(8).html());
            txtRemarks.val(tds.eq(9).html())
            if (tds.eq(10).attr("data-val") == "Active") {
                chkStatestatus.prop("checked", true);
            }
        })

        $("#btnSubmitDealer").click(function (e) {
            e.preventDefault();
            if (validateDealer()) {

                disable("#btnSubmitDealer");
                var data = JSON.stringify({
                    DealerId: hdfDealerId.val(),
                    DealerName: txtDealerName.val(),
                    Address: txtAddress.val(),
                    TelephoneNo: txtTelephone.val(),
                    MobileNo: txtMobile.val(),
                    Email: txtEmail.val(),
                    ContactPersonName: txtContactPersonName.val(),
                    ContactPersonMobile: txtContactPersonMobile.val(),
                    Remarks: txtRemarks.val(),
                    IsActive: chkStatestatus.is(":checked")
                })

                $.when(postReqWithToken(dataType.applicationJson, dataType.json, type.post, '/SalesAndDistribution/SaveDealer', data, getToken())).then(function (res,status) {
                    if (res === true && status === "success") {
                        toastrSuccessAlert(execuStatus.successSave);
                        $("#modalDealer").modal("toggle");
                        clearDealerCtrl();
                        loadDealerDataTable();
                    }
                    else {
                        toastrErrorAlert(execuStatus.fail);
                    }
                    enable("#btnSubmitDealer");
                }).fail(function (error) {
                    consoleLog(error);
                    enable("#btnSubmitDealer");
                })
            }
        })

        // Category //
        //data-category-edit
        $("#btnAddNewCategory").click(function (e) {
            e.preventDefault();
            OpenModalCategory();
        })

        function OpenModalCategory() {
            $("#modalCategory").modal("show");
            $("#spanModalHeadCategory").text("Add New Dealer");
            $("#spanSaveTextCategory").text("Save");
            clearCategoryCtrl();
        }

        function clearCategoryCtrl() {
            hdfCategoryId.val("0");
            txtCategoryName.val('');
            txtCategoryRemarks.val('');
            chkCategoryStatestatus.prop("checked", false);
        }

        function validateCategory() {
            var isValid = true;
            $(".error").addClass("hide");
            if ($.trim(txtCategoryName.val()) == '') {
                $(".required-category").removeClass("hide");
                isValid = false;
            }
            else {
                // Duplicate Checking ...
            }
            return isValid;
        }

        $(document).on('click', '.data-category-edit', function (e) {
            e.preventDefault();
            OpenModalCategory();
            $("#spanModalHeadCategory").text("Update Category");
            $("#spanSaveTextCategory").text("Update");
            var tds = $(this).parent().parents('tr').children('td');
            hdfCategoryId.val(tds.eq(1).html());
            txtCategoryName.val(tds.eq(2).html());
            txtCategoryRemarks.val(tds.eq(3).html())
            if (tds.eq(4).attr("data-val") == "Active") {
                chkCategoryStatestatus.prop("checked", true);
            }
        })

        $("#btnSubmitCategory").click(function (e) {
            e.preventDefault();
            if (validateCategory()) {

                var data = JSON.stringify({
                    CategoryId: TryParseInt(hdfCategoryId.val(),0),
                    CategoryName: txtCategoryName.val(),
                    Remarks: txtCategoryRemarks.val(),
                    IsActive : chkCategoryStatestatus.is(":checked")
                })

                disable("#btnSubmitCategory");
                $.when(postReqWithToken(dataType.applicationJson, dataType.json, type.post, '/SalesAndDistribution/SaveCategory', data, getToken())).then(function (res, status) {
                    if (res === true && status === "success") {
                        toastrSuccessAlert(execuStatus.successSave);
                        $("#modalCategory").modal("toggle");
                        clearCategoryCtrl();
                        loadCategoryDataTable();
                    }
                    else {
                        toastrErrorAlert(execuStatus.fail);
                    }
                    enable("#btnSubmitCategory");
                }).fail(function (error) {
                    consoleLog(error);
                    enable("#btnSubmitCategory");
                })
            }
        })
    </script>
}

