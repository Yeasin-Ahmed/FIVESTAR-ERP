        $("#btnSubmitBrand").click(function (e) {
            e.preventDefault();
            if (validateBrand()) {

                var categories = [];
                if (ddlBrandCategories.val().length > 0) {
                    $.each(ddlBrandCategories.val(), function (index, item) {
                        categories.push(item);
                    })
                }

