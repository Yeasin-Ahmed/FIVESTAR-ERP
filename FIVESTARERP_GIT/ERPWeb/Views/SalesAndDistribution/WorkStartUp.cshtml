            txtTelephone.val('');
            txtMobile.val('');
            txtEmail.val('');
            txtContactPersonName.val('');
            txtContactPersonMobile.val('');
            txtRemarks.val('');
            chkStatestatus.prop("checked", false);
            txtDealerName.focus();
        }

        function validateDealer() {
            var isValid = true;
            $(".error").addClass("hide");
            if ($.trim(txtDealerName.val()) == '') {
                $(".required-dealerName").removeClass("hide");
                isValid = false;
            }
            if ($.trim(txtAddress.val()) == '') {
                $(".required-address").removeClass("hide");
                isValid = false;
            }
            return isValid;
        }

        $(document).on('click', '.data-dealer-edit', function (e) {
            e.preventDefault();
            OpenModalDealer();
            $("#spanModalHeadDealer").text("Update Dealer");
            $("#spanSaveTextDealer").text("Update");
            var tds = $(this).parent().parents('tr').children('td');
            hdfDealerId.val(tds.eq(1).html());
            txtDealerName.val(tds.eq(2).html());
            txtAddress.val(tds.eq(3).html());
            txtTelephone.val(tds.eq(4).html());
            txtMobile.val(tds.eq(5).html());
            txtEmail.val(tds.eq(6).html());
            txtContactPersonName.val(tds.eq(7).html());
            txtContactPersonMobile.val(tds.eq(8).html());
            txtRemarks.val(tds.eq(9).html())
            if (tds.eq(10).attr("data-val") == "Active") {
                chkStatestatus.prop("checked", true);
            }
        })

        $("#btnSubmitDealer").click(function (e) {
            e.preventDefault();
            if (validateDealer()) {

                disable("#btnSubmitDealer");
                var data = JSON.stringify({
                    DealerId: hdfDealerId.val(),
                    DealerName: txtDealerName.val(),
                    Address: txtAddress.val(),
                    TelephoneNo: txtTelephone.val(),
                    MobileNo: txtMobile.val(),
                    Email: txtEmail.val(),
                    ContactPersonName: txtContactPersonName.val(),
                    ContactPersonMobile: txtContactPersonMobile.val(),
                    Remarks: txtRemarks.val(),
                    IsActive: chkStatestatus.is(":checked")
                })

                $.when(postReqWithToken(dataType.applicationJson, dataType.json, type.post, '/SalesAndDistribution/SaveDealer', data, getToken())).then(function (res, status) {
                    if (res === true && status === "success") {
                        toastrSuccessAlert(execuStatus.successSave);
                        $("#modalDealer").modal("toggle");
                        clearDealerCtrl();
                        loadDealerDataTable();
                    }
                    else {
                        toastrErrorAlert(execuStatus.fail);
                    }
                    enable("#btnSubmitDealer");
