                <div class="modal-header alert-secondary">
                    <h4 id="modalHeading" class="modal-title"><span id="spanModalHeadModel">Add New Model</span></h4>
                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span></button>
                </div>
                <div class="modal-body">
                    <form id="frmBrand">
                        <input type="hidden" name="hdfModelId" id="hdfModelId" />
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label for="txtModelName" class="control-label font-weight-bold">
                                    Model
                                    <div class="checkbox d-inline icheck-emerland">
                                        <input type="checkbox" name="chkModelStatestatus" id="chkModelStatestatus" />
                                        <label for="chkModelStatestatus" class="text-success">
                                            Active
                                        </label>
                                    </div>
                                </label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-mobile"></i>
                                        </span>
                                    </div>
                                    <input type="text" name="txtModelName" value="" id="txtModelName" class="form-control form-control-sm" />
                                </div>
                                <span class="error hide required-color font-weight-bold">Input Color!</span>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="txtColorRemarks" class="control-label font-weight-bold">Remarks</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-comment"></i>
                                        </span>
                                    </div>
                                    <input type="text" name="txtColorRemarks" value="" id="txtColorRemarks" class="form-control form-control-sm" />
                                </div>
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                @Html.DropDownList("ddlModelColors", (IEnumerable<SelectListItem>)ViewBag.ddlColors, null, new { @class = "form-control form-control-sm js-example-placeholder-multiple js-states form-control", @multiple = "multiple" })
                            </div>
                        </div>
                    </form>
                </div>
                <div class="modal-footer btn-light">
                    <button type="button" class="btn btn-danger btn-flat float-right" data-dismiss="modal" data-target="#"><i class="fas fa-times"></i> Cancel </button>
                    <button type="submit" class="btn btn-success btn-flat float-right" id="btnSubmitModel"><i class="far fa-save"></i> <span id="spanSaveTextColor"> Save</span> </button>
                </div>
            </div>
        </div>
    </div>
</div>

