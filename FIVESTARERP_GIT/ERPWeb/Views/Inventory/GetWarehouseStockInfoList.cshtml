@section scripts{

    <script type="text/javascript">
        var ddlWarehouse = $("#ddlWarehouse");
        var ddlItemType = $("#ddlItemType");
        var ddlItem = $("#ddlItem");
        var txtInQuantity = $("#txtInQuantity");
        var txtRemarks = $("#txtRemarks");
        var unitName = $("#unitName");
        var txtLessOrEq = $("#txtLessOrEq");
        var ddlModelName = $("#ddlModelName");
        var pageNo = 1;

        $(document).ready(function () {
            //Initialize Select2 Elements
            $('.select2').select2();

            //Initialize Select2 Elements
            $('.select2bs4').select2({
                theme: 'bootstrap4'
            });

            if ('@tab' =="IQC") {
                $("#custom-tabs-two-IQCItemReqInfoList-tab").trigger('click');
            }
            if ('@tab' == "IQCReturn") {
                $("#custom-tabs-two-IQCItemReturnList-tab").trigger('click');
            }
        })

        $(document).on('change', '.date-datePicker', function (e) {
            var dateval = $(this).val();
            console.log(dateval);
        })

        //casced dropdown load for itemtype
        ddlWarehouse.change(function () {
            $("#unitName").text('');
            clearDropdown("ddlItemType");
            clearDropdown("ddlItem");
            if (ddlWarehouse.val() != "") {
                LoadDropDown('/Common/GetItemTypeForDDL', 'POST', ddlItemType, JSON.stringify({ warehouseId: ddlWarehouse.val() }));
            }
            LoadDataTable();
        })

        //casced dropdown load for item
        ddlItemType.change(function () {
            $("#unitName").text('');
            clearDropdown("ddlItem");
            if (ddlItemType.val() != "") {
                LoadDropDown('/Common/GetItemForDDL', 'POST', ddlItem, JSON.stringify({ itemTypeId: ddlItemType.val() }));
            }
            LoadDataTable();
        })

        ddlItem.change(function () {
            LoadDataTable();
        })

        ddlModelName.change(function () {
            LoadDataTable();
        })

        txtLessOrEq.keyup(function () {
            LoadDataTable();
        })

        function LoadDataTable() {
            var data = { flag: "search",warehouseId: TryParseInt(ddlWarehouse.val(), 0), modelId: TryParseInt(ddlModelName.val(), 0), itemTypeId: TryParseInt(ddlItemType.val(), 0), itemId: TryParseInt(ddlItem.val(), 0), lessOrEq: txtLessOrEq.val(), page: pageNo };
            $.when(getReqWithData('html', 'GET', '/Inventory/GetWarehouseStockInfoList', data)).then(function (res, status) {
                console.log(status);
                if (status === "success") {
                    $("#dataContainer").fadeOut('500', function () {
                        $("#dataContainer").empty();
                        $("#dataContainer").append(res).fadeIn('500');
                    });
                }
            }).fail(function (error) {
                console.log(error);
            });
            pageNo = 1;
        }

        // Pagination
        $(document).on('click', 'a.page-link', function (e) {
            e.preventDefault();
            if (!$(this).hasClass('current-page')) {
                //alert('Warehouse');
                if ($(this).parent().parents('div').hasClass('warehouseStock')) {
                    pageNo = $(this).attr('data-page-no');
                    //console.log('Page No');
                    //console.log(!isNaN(pageNo));
                    LoadDataTable();
                }
            }
    })

    function OpenModalExcel() {
        $("#modelDataContainer").empty();
        $("#modalExcelImport").modal("show");
    }

    $("#btnExcelImport").click(function (e) {
        e.preventDefault();
        $.when(getReqWithData('html', 'GET', '/Inventory/CreateExcel', null)).then(function (res, status) {
            if (status == "success") {
                OpenModalExcel();
                $("#modelContainer2").empty();
                $("#modelContainer2").append(res);
            }
        }).fail(function (error) {
            console.log(error);
        })
    })

    var FileUpload = $(".FileUpload");

        $(document).on('click', '.btnExcelSubmit', function (e) {
            e.preventDefault();
            var excelFile = $(".FileUpload").prop('files')[0];
            var formData = new FormData();
            formData.append("FileUpload", excelFile);
            $.when(postReqWithFile(dataType.json, type.post, '/Inventory/UploadExcel', formData, getToken())).then(function (res,status) {
                    console.log(res);
                    console.log(status);
                    if (res === true && status ==='success') {
                        toastrSuccessAlert(execuStatus.successSave);
                        $("#modalExcelImport").modal("toggle").toggle("fast");
                        //removeTableRow("#tblWarehouseStockInfo tbody", index);
                        setTimeout(function () {
                        LoadDataTable();
                        @*redirectPage('@Url.Action("GetWarehouseStockInfoList")');*@
                        }, 1500);
                }
                    else {
                        toastrErrorAlert(execuStatus.fail);
                    }
            })
        })
        function redirectPage(page) {
            window.location.replace(page);
        }
</script> 

    <script type="text/javascript">
        var ddlWarehouseForIQCReq = $("#ddlWarehouseForIQCReq");
        var ddlStateStatus = $("#ddlStateStatus");
        var ddlModelNameForIQCReq = $("#ddlModelNameForIQCReq");
        var pageNoForIQCReq = 1;


        $(document).ready(function () {
            //Initialize Select2 Elements
            $('.select2').select2();
            $("#btnReacive").hide();
            //Initialize Select2 Elements
            $('.select2bs4').select2({
                theme: 'bootstrap4'
            });
        })

        $(document).on('change', '.date-datePicker', function (e) {
            var dateval = $(this).val();
            console.log(dateval);
        })

        //Modal
        var hdfRowIndex = $("#hdfRowIndex");
        var hdfInfoId = $("#hdfInfoId");

        function OpenModal() {
            $("#modelDataContainer").empty();
            hdfInfoId.val('0');
            hdfRowIndex.val("");
            $("#modalIQCReqDetails").modal("show");

        }

        $(document).on("click", ".data-details", function (e) {

            e.preventDefault();
            var id = $(this).attr("data-details");

            var data = {
                reqInfoId: TryParseInt(id, 0)
            };
            var idx = $(this).parent().parents('tr').index();

            console.log(data);
            //return;
            $.when(getReqWithData('html', 'GET', '/Inventory/GetIQCItemReqDetailList', data)).then(function (res, status) {
                if (status == "success") {
                    OpenModal();
                    hdfInfoId.val(data.reqInfoId);
                    hdfRowIndex.val(idx);
                    $("#modelContainer1").empty();
                    $("#modelContainer1").append(res);
                    $(".btnReacive").addClass('hide');
                }
            }).fail(function (error) {
                console.log(error);
            })
        })

        ddlWarehouseForIQCReq.change(function () {

            LoadDataTableForIQCReq();
        })

        ddlModelNameForIQCReq.change(function () {
            LoadDataTableForIQCReq();
        })

        ddlStateStatus.change(function () { LoadDataTableForIQCReq(); })

        //txtLessOrEq.keyup(function () {
        //    LoadDataTable();
        //})

        function LoadDataTableForIQCReq() {
            var data = { WarehouseId: TryParseInt(ddlWarehouseForIQCReq.val(), 0), modelId: TryParseInt(ddlModelNameForIQCReq.val(), 0), /*ItemTypeId: TryParseInt(ddlItemType.val(), 0), ItemId: TryParseInt(ddlItem.val(), 0), lessOrEq: txtLessOrEq.val(),*/stateStatus: ddlStateStatus.val(), page: pageNoForIQCReq };
            $.when(getReqWithData('html', 'GET', '/Inventory/GetIQCReqInfoListForWarehouse', data)).then(function (res, status) {
                console.log(status);
                if (status === "success") {
                    $("#dataContainer2").fadeOut('500', function () {
                        $("#dataContainer2").empty();
                        $("#dataContainer2").append(res).fadeIn('500');
                    });
                }
            }).fail(function (error) {
                console.log(error);
            });
            pageNoForIQCReq = 1;
        }

        // Pagination
        $(document).on('click', 'a.page-link', function (e) {
            e.preventDefault();
            if (!$(this).hasClass('current-page')) {
                //alert('Page Clicked');
                if ($(this).parent().parents('div').hasClass('IQCReqPage')) {
                    //alert('Page Found');
                    pageNoForIQCReq = $(this).attr('data-page-no');
                    //console.log('Page No');
                    //console.log(!isNaN(pageNo));
                    LoadDataTableForIQCReq();
                }

            }
        })

        $("#btnResetForIQCReq").click(function (e) {
            e.preventDefault();
            ddlItem.val('');
            ddlItem.trigger("change");
            ddlWarehouseForIQCReq.val("");
            ddlWarehouseForIQCReq.trigger("change");
            ddlModelNameForIQCReq.val('');
            ddlModelNameForIQCReq.trigger("change");
            ddlStateStatus.val('');
            ddlStateStatus.trigger("change");
            $("#tblIQCReq tbody").empty();
        })

    </script>

