        ddlStateStatusForIQCReturn.change(function () { LoadDataTableForIQCReturn(); })

        function LoadDataTableForIQCReturn() {
            var data = { WarehouseId: TryParseInt(ddlWarehouseForIQCReturn.val(), 0), modelId: TryParseInt(ddlModelNameForIQCReturn.val(), 0),stateStatus: ddlStateStatusForIQCReturn.val(), page: pageNoForIQCReturn };
            $.when(getReqWithData('html', 'GET', '/Inventory/GetIQCReturnDataInfoListForWarehouse', data)).then(function (res, status) {
                console.log(status);
                if (status === "success") {
                    $("#dataContainer3").fadeOut('500', function () {
                        $("#dataContainer3").empty();
                        $("#dataContainer3").append(res).fadeIn('500');
                    });
                }
            }).fail(function (error) {
                console.log(error);
            });
            pageNoForIQCReturn = 1;
        }

        // Pagination
        $(document).on('click', 'a.page-link', function (e) {
            e.preventDefault();
            if (!$(this).hasClass('current-page')) {
                //alert('Page Clicked');
                if ($(this).parent().parents('div').hasClass('IQCItemReturnInfoForWarehouse')) {
                    //alert('Page Found');
                    pageNoForIQCReturn = $(this).attr('data-page-no');
                    LoadDataTableForIQCReturn();
                }

            }
        })

        $("#btnResetForIQCReturn").click(function (e) {
            e.preventDefault();
            ddlItem.val('');
            ddlItem.trigger("change");
            ddlWarehouseForIQCReturn.val("");
            ddlWarehouseForIQCReturn.trigger("change");
            ddlModelNameForIQCReturn.val('');
            ddlModelNameForIQCReturn.trigger("change");
            ddlStateStatusForIQCReturn.val('');
            ddlStateStatusForIQCReturn.trigger("change");
            $("#tblIQCItemReqInfoData tbody").empty();
        })

        $(document).on('click', ".btnReacive-Return", function (e) {
            e.preventDefault();
            fnIQCStockChange(hdfInfoId.val(), hdfRowIndex.val(), reqStatus.receive_return);
        })

        function fnIQCStockChange(id, index, status) {
            if (id > 0) {

                var data = JSON.stringify({ reqId: id, status: status });
                $.when(postReqWithToken(dataType.applicationJson, dataType.json, 'POST', '/Inventory/SaveWarehouseStockInByIQCReturn', data, getToken())).then(function (res, status) {
                    if (status == "success" && res === true) {
                        toastrSuccessAlert(execuStatus.successSave);
                        $("#modalIQCReqDetails").modal("toggle").toggle("fast");
                        hdfInfoId.val('0');
                        hdfRowIndex.val("");
                        removeTableRow("#tblIQCItemReqInfoData tbody", index);
                        setTimeout(function () {
                            LoadDataTableForIQCReturn();
                            redirectPage('@Url.Action("GetWarehouseStockInfoList",new {tab="IQCReturn" })');
                        }, 1500);
                    }
                }).fail(function (error) {
                    console.log(error);
                    $(".toastrDefaultError").trigger('click');
                })
            }
        }
    </script>
}