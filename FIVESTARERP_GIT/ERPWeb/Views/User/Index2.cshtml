@if (User.RoleName == "Warehouse")
{
    <div class="row">

        <div class="col-md-6 col-6">
            <div class="card shadow card-gray-dark">
                <div class="card-header border-transparent">
                    <h3 class="card-title">Current Requsition</h3>
                    <div class="card-tools">
                        <button type="button" class="btn btn-tool" data-card-widget="collapse">
                            <i class="fas fa-minus"></i>
                        </button>
                        <button type="button" class="btn btn-tool" data-card-widget="remove">
                            <i class="fas fa-times"></i>
                        </button>
                    </div>
                </div>
                <div class="card-body p-0" style="max-height:200px;overflow-y:scroll">
                    @if (((IEnumerable<DashboardApprovedRequsitionViewModel>)ViewBag.DashboardCurrentViewModel).Count() > 0)
                    {
                        <table class="table table-sm table-valign-middle text-sm">
                            <thead>
                                <tr>
                                    <th class="hide"></th>
                                    <th class="hide"></th>
                                    <th class="text-center">JobOrder</th>
                                    <th class="text-center">ReqisitionCode</th>
                                    <th class="text-center">ReqisitionDate</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var app in (IEnumerable<DashboardApprovedRequsitionViewModel>)ViewBag.DashboardCurrentViewModel)
                                {
                                    <tr>
                                        <td class="hide">@app.JodOrderId</td>
                                        <td class="hide">@app.RequsitionInfoForJobOrderId</td>
                                        <td class="text-center">@app.JobOrderCode</td>
                                        <td class="text-center"><a href="#" class="text-bold btnStockOut" data-item-current="@app.RequsitionInfoForJobOrderId">@app.RequsitionCode</a></td>
                                        <td class="text-center">@app.EntryDate.Value.ToString("dd-MMM-yyyy")</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    }
                    else
                    {
                        <div class="text-center text-indigo" style="width:200px;height:200px">
                            <div style="position:absolute;top:40%;left:35%">
                                <i class="fas fa-address-card fa-5x"></i>
                                <p class="text-dark text-bold">No Current Requsition</p>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>

        <div class="col-md-6 col-6">
            <div class="card shadow card-gray-dark">
                <div class="card-header border-transparent">
                    <h3 class="card-title">Pending Requsition</h3>
                    <div class="card-tools">
                        <button type="button" class="btn btn-tool" data-card-widget="collapse">
                            <i class="fas fa-minus"></i>
                        </button>
                        <button type="button" class="btn btn-tool" data-card-widget="remove">
                            <i class="fas fa-times"></i>
                        </button>
                    </div>
                </div>
                <div class="card-body p-0" style="max-height:200px;overflow-y:scroll">
                    @if (((IEnumerable<DashboardApprovedRequsitionViewModel>)ViewBag.DashboardPendingViewModel).Count() > 0)
                    {
                        <table class="table table-sm table-valign-middle text-sm">
                            <thead>
                                <tr>
                                    <td class="hide"></td>
                                    <td class="hide"></td>
                                    <th class="text-center">JobOrder</th>
                                    <th class="text-center">ReqisitionCode</th>
                                    <th class="text-center">ReqisitionDate</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var app in (IEnumerable<DashboardApprovedRequsitionViewModel>)ViewBag.DashboardPendingViewModel)
                                {
                                    <tr>
                                        <td class="hide">@app.JodOrderId</td>
                                        <td class="hide">@app.RequsitionInfoForJobOrderId</td>
                                        <td class="text-center">@app.JobOrderCode</td>
                                        <td class="text-center"><a href="#" class="text-bold btnStockOutPending" data-item-pending="@app.RequsitionInfoForJobOrderId">@app.RequsitionCode</a></td>
                                        <td class="text-center">@app.EntryDate.Value.ToString("dd-MMM-yyyy")</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    }
                    else
                    {
                        <div class="text-center text-info" style="width:200px;height:200px">
                            <div style="position:absolute;top:40%;left:35%">
                                <i class="fas fa-hand-holding-heart fa-5x"></i>
                                <p class="text-dark text-bold">No Pending Requsition</p>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>

    </div>
    <div class="row">

        <div class="col-md-6 col-6">
            <div class="card shadow card-gray-dark">
                <div class="card-header border-transparent">
                    <h3 class="card-title">Other Branch Requsition</h3>
                    <div class="card-tools">
                        <button type="button" class="btn btn-tool" data-card-widget="collapse">
                            <i class="fas fa-minus"></i>
                        </button>
                        <button type="button" class="btn btn-tool" data-card-widget="remove">
                            <i class="fas fa-times"></i>
                        </button>
                    </div>
                </div>
                <div class="card-body p-0" style="max-height:200px;overflow-y:scroll">
                    @if (((IEnumerable<DashboardApprovedRequsitionViewModel>)ViewBag.AnotherBranchRequsition).Count() > 0)
                    {
                        <table class="table table-sm table-valign-middle text-sm">
                            <thead>
                                <tr>
                                    <th class="hide"></th>
                                    <th class="hide"></th>
                                    <th class="text-center">JobOrder</th>
                                    <th class="text-center">ReqisitionCode</th>
                                    <th class="text-center">Status</th>
                                    <th class="text-center">ReqisitionDate</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var another in (IEnumerable<DashboardApprovedRequsitionViewModel>)ViewBag.AnotherBranchRequsition)
                                {
                                    badge = another.StateStatus == "Pending" ? "badge bg-warning" : (another.StateStatus == "Approved" ? "badge bg-success" : "badge bg-primary");
                                    <tr>
                                        <td class="hide">@another.JodOrderId</td>
                                        <td class="hide">@another.RequsitionInfoForJobOrderId</td>
                                        <td class="text-center">@another.JobOrderCode</td>
                                        <td class="text-center"><a href="#" class="text-bold btnStockOutOther" data-item-other="@another.RequsitionInfoForJobOrderId">@another.RequsitionCode</a></td>
                                        <td class="text-center" data-val="@another.StateStatus">
                                            <span class="@badge">@another.StateStatus</span>
                                        </td>
                                        <td class="text-center">@another.EntryDate.Value.ToString("dd-MMM-yyyy")</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    }
                    else
                    {
                        <div class="text-center text-fuchsia" style="width:200px;height:200px">
                            <div style="position:absolute;top:40%;left:35%">
                                <i class="fas fa-heart-broken fa-5x"></i>
                                <p class="text-dark text-bold">No Other Branch Requsition</p>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>

        <div class="col-md-6 col-6">
            <div class="card shadow card-gray-dark">
                <div class="card-header border-transparent">
                    <h3 class="card-title">Parts Return(TS end)</h3>
                    <div class="card-tools">
                        <button type="button" class="btn btn-tool" data-card-widget="collapse">
                            <i class="fas fa-minus"></i>
                        </button>
                        <button type="button" class="btn btn-tool" data-card-widget="remove">
                            <i class="fas fa-times"></i>
                        </button>
                    </div>
                </div>
                <div class="card-body p-0" style="max-height:200px;overflow-y:scroll">
                    @if (((IEnumerable<DashbordTsPartsReturnViewModel>)ViewBag.ReturnPartsViewModel).Count() > 0)
                    {
                        <table class="table table-sm table-valign-middle text-sm">
                            <thead>
                                <tr>
                                    <th class="hide"></th>
                                    <th class="hide"></th>
                                    <th class="text-center">JobOrder</th>
                                    <th class="text-center">ReqisitionCode</th>
                                    <th class="text-center">ReqisitionDate</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var stock in (IEnumerable<DashbordTsPartsReturnViewModel>)ViewBag.ReturnPartsViewModel)
                                {
                                    <tr>
                                        <td class="hide">@stock.JobOrderId</td>
                                        <td class="hide">@stock.ReturnInfoId</td>
                                        <td class="text-center">@stock.JobOrderCode</td>
                                        <td class="text-center"><a href="#" class="text-bold btnPartsReturn" data-item-val="@stock.ReturnInfoId.ToString()">@stock.RequsitionCode</a></td>
                                        <td class="text-center">@stock.EntryDate.Value.ToString("dd-MMM-yyyy")</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    }
                    else
                    {
                        <div class="text-center text-fuchsia" style="width:200px;height:200px">
                            <div style="position:absolute;top:40%;left:35%">
                                <i class="fas fa-atom fa-5x"></i>
                                <p class="text-dark text-bold">No Parts Return</p>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>

    </div>
}


<div class="modal fade" id="modalTsStockReturn" role="dialog" tabindex="-1" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header alert-secondary">
                <h4 id="modalHeading" class="modal-title"><span id="spanModalHead">Return Stock</span></h4>
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span></button>
            </div>
