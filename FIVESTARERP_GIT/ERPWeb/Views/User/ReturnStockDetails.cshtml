@model IEnumerable<ERPBO.FrontDesk.ViewModels.TsStockReturnDetailViewModel>
@{
    int sl = 0;
}
<table class="table table-bordered table-sm table-striped" id="tblWarehouseStockInfo">
    <thead class="btn-dark">
        <tr class="text-center">
            <th>#SL</th>
            <th class="hide"></th>
            <th class="hide"></th>
            <th class="hide"></th>
            <th>PartsName</th>
            <th>Quantity</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var details in Model)
        {
            sl = sl + 1;
            <tr>
                <td class="text-center text-bold">@sl</td>
                <td class="hide">@details.ReturnInfoId</td>
                <td class="hide">@details.ReturnDetailId</td>
                <td class="hide">@details.PartsId</td>
                <td>@details.PartsName</td>
                <td>@details.Quantity</td>
            </tr>
        }
        @if (Model.Count() == 0)
        {
            <tr class="text-center empty-row">
                <td colspan="13">
                    No data available in table
                </td>
            </tr>
        }
    </tbody>
</table>